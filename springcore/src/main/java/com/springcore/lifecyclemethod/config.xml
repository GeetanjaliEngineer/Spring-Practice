<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.springcore.lifecyclemethod.Samosa" name="s1"
		init-method="hey" destroy-method="bye">
		<property name="price" value="8" />
	</bean>

	<bean class="com.springcore.lifecyclemethod.Pepsi" name="p1"
		init-method="initPepsi" destroy-method="destroyPepsi">
		<property name="price" value="5" />
	</bean>

</beans>
